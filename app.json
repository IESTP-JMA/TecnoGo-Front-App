{
  "expo": {
    "name": "Tecno Go",
    "slug": "tecno-go",
    "version": "1.0.0-alpha",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "automatic",
    "scheme": "tecnogo",
    "androidStatusBar": {
      "backgroundColor": "#064E3B",
      "barStyle": "light-content"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#000000"
      },
      "package": "com.iestpjma.tecnogo"
    },
    "plugins": [
      [
        "expo-splash-screen",
        {
          "backgroundColor": "#064E3B",
          "image": "./assets/adaptive-icon.png", 
          "imageWidth": 300
        }
      ],
      "expo-router",
      [
        "expo-font",
        {
          "fonts": ["./assets/fonts/OpenDyslexic-Bold.otf"]
        }
      ],
      "expo-secure-store",
      "expo-sqlite"
    ],
    "extra": {
      "eas": {
        "projectId": "a4752093-0ad8-4117-8065-320eaed9618a"
      }
    },
    "owner": "yerald",
    "newArchEnabled": true
  }
}
