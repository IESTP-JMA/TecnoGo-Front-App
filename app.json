{
  "expo": {
    "name": "TecnoGO",
    "slug": "tecno-go",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "automatic",
    "scheme": "tecnogo",
    "splash": {
      "image": "./assets/splash.jpeg",
      "resizeMode": "cover",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.ietsppjma.tecnogo"
    },
    "plugins": [
      "expo-router",
      "expo-font",
      "expo-secure-store"
    ],
    "extra": {
      "eas": {
        "projectId": "a4752093-0ad8-4117-8065-320eaed9618a"
      }
    },
    "owner": "yerald",
    "newArchEnabled": true  

  }
}
