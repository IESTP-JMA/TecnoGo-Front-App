{
  "expo": {
    "name": "TecnoGO",
    "slug": "tecno-go",
    "version": "1.0.0-alpha",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "automatic",
    "scheme": "tecnogo",
    "splash": {
      "backgroundColor": "#006634",
      "image": "./assets/splash.png",
      "resizeMode":"cover"
    },
    "androidStatusBar": {
      "backgroundColor": "#064E3B",
      "barStyle": "light-content"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#881337"
      },
      "package": "com.iestpjma.tecnogo"
    },
    "plugins": [
      "expo-router",
      "expo-font",
      "expo-secure-store",
      "expo-sqlite"
    ],
    "extra": {
      "eas": {
        "projectId": "a4752093-0ad8-4117-8065-320eaed9618a"
      }
    },
    "owner": "yerald",
    "newArchEnabled": true
  }
}
